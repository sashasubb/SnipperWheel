<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/rotate_btn_enabled"/>
            <corners
                android:radius="20dp"
                />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/rotate_btn_disabled"/>
            <corners
                android:radius="20dp"
                />
        </shape>
    </item>
</selector>